{
  "name": "vue-webpack-ts-simple",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "paul van bladel <paul.vanbladel@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env router=product sso=local webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "router-dev": "node build/dev-server.js router=development sso=local",
    "router-pro": "node build/dev-server.js router=product",
    "unit": "cross-env karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "unit-debug": "karma start test/unit/karma.conf.js --browsers=Chrome --auto-watch --no-colors",
    "lint": "npx eslint --ext .ts,.vue src",
    "watch": "cross-env NODE_ENV=dev webpack --watch  --color --config build/webpack.prod.conf.js",
    "build": "npm run build:dev",
    "product": "npm run build:prod",
    "build:dev": "cross-env NODE_ENV=dev node build/build.js",
    "build:prod": "cross-env NODE_ENV=production node build/build.js",
    "dll": "rm -rf ./dll&&npx webpack --config build/webpack.dll.conf.js --progress ",
    "log": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.conf.js --colors --profile --progress",
    "integration": "cross-env NODE_ENV=integration node build/build.js"
  },
